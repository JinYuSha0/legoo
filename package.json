{
  "name": "rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack-dev-server --config ./web/webpack.config.js --mode development",
    "build:android": "react-native bundle --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android --assets-dest ./android/app/src/main/res --dev false --reset-cache",
    "build:ios": "react-native bundle --entry-file index.js --bundle-output ./ios/main.jsbundle --platform ios --assets-dest ./ios --dev false --reset-cache",
    "build:web": "webpack --config ./web/webpack.config.js --mode production",
    "build:packages": "turbo run build",
    "watch:packages": "turbo run watch --parallel",
    "publish-packages": "turbo run build && changeset version && changeset publish",
    "proxy": "set HTTP_PROXY=http://127.0.0.1:7890 && set HTTPS_PROXY=http://127.0.0.1:7890"
  },
  "dependencies": {
    "@legoo/headless": "workspace:*",
